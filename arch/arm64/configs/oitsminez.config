# oItsMineZKernel config file (Make some change from ExtremeKernel config)
# This file gets merged with the device-specific defconfig on kernel compilation
# Kernel defconfigs are left stock with minimal edits while this file includes all of the major modifications done to the kernel config

# Set custom kernel version
CONFIG_LOCALVERSION="-oItsMineZKernel-OneUI4-v1-S10"
# CONFIG_LOCALVERSION_AUTO is not set

# Enable OverlayFS
CONFIG_OVERLAY_FS=y

# KernelSU
# CONFIG_KSU is not set
# CONFIG_KSU_DEBUG is not set

# Performance enchantment
CONFIG_CC_OPTIMIZE_FOR_PERFORMANCE_O3=y
# CONFIG_CC_OPTIMIZE_FOR_SIZE is not set
# CONFIG_CC_OPTIMIZE_FOR_PERFORMANCE is not set

# Spoof CMDLine properties for Play Integrity API / SafetyNet attestation
CONFIG_PROC_SPOOF_CMDLINE=y

# Enable fingerprint enchantment
CONFIG_FINGERPRINT_BOOST=y

# Enable misc kernel features
CONFIG_CPU_FREQ_GOV_POWERSAVE=y
CONFIG_CPU_FREQ_GOV_USERSPACE=y
CONFIG_CPU_FREQ_GOV_ONDEMAND=y
CONFIG_CPU_FREQ_GOV_CONSERVATIVE=y
CONFIG_TCP_CONG_WESTWOOD=y
CONFIG_TCP_CONG_HTCP=y
CONFIG_IOSCHED_BFQ=y
CONFIG_IP_NF_TARGET_TTL=y
CONFIG_CRYPTO_LZ4=y
CONFIG_CRYPTO_ZSTD=y
CONFIG_TOOLS_SUPPORT_RELR=y
CONFIG_ARCH_HAS_RELR=y
CONFIG_RELR=y
CONFIG_LLVM_POLLY=y

# Enable force Dex WQHD by default
CONFIG_DISPLAYPORT_DEX_FORCE_WQHD=y

# Enable NTFS by default
CONFIG_NTFS_FS=y
CONFIG_NTFS_RW=y
# CONFIG_NTFS_DEBUG is not set

# Enable Boeffla WL blocker by default
CONFIG_BOEFFLA_WL_BLOCKER=y

# Enable Wireguard by default
CONFIG_WIREGUARD=y
CONFIG_NET_UDP_TUNNEL=y
CONFIG_BOEFFLA_WL_BLOCKER=y
# CONFIG_GTP is not set
# CONFIG_GENEVE is not set
# CONFIG_WIREGUARD_DEBUG is not set

# Enable power efficient workqueues
CONFIG_WQ_POWER_EFFICIENT_DEFAULT=y

# EOF

